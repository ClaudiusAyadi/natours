{
	"name": "natours",
	"version": "1.0.0",
	"description": "Node + Express + MongoDB",
	"main": "app.js",
	"author": "Claudius A.",
	"license": "MIT",
	"scripts": {
		"watch": "esbuild ./public/js/index.js --bundle --outfile=./public/js/bundle.js --watch",
		"build": "esbuild ./public/js/index.js --bundle --minify --outfile=./public/js/bundle.js",
		"dev": "concurrently --kill-others \"pnpm watch\" \"nodemon server.js\"",
		"prod": "set NODE_ENV=production&&nodemon server.js",
		"start": "node server.js",
		"debug": "ndb server.js"
	},
	"dependencies": {
		"@stripe/stripe-js": "^2.4.0",
		"axios": "^1.6.5",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"core-js": "^3.35.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"html-to-text": "^9.0.5",
		"jsonwebtoken": "^9.0.2",
		"mapbox-gl": "^3.1.0",
		"mongodb": "^6.3.0",
		"mongoose": "^8.0.3",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"ndb": "^1.1.5",
		"nodemailer": "^6.9.8",
		"pug": "^3.0.2",
		"regenerator-runtime": "^0.14.1",
		"sharp": "0.32.6",
		"slugify": "^1.6.6",
		"stripe": "^14.14.0",
		"validator": "^13.11.0"
	},
	"devDependencies": {
		"@prettier/plugin-pug": "^3.0.0",
		"concurrently": "^8.2.2",
		"esbuild": "^0.19.12",
		"eslint": "^8.56.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.12.4",
		"prettier": "^3.1.1"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
